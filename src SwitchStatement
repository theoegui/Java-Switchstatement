# Java-Switchstatement

class Switchstatement{
	public static void main(String args[]){
		int age;
		age = 3;
		
		switch (age){
		case 1:
			System.out.println("Your too young");
			break;
		case 2:
			System.out.println("Your too perfect");
			break;
		case 3:
			System.out.println("Your too old");
			break;
		case 4:
			System.out.println("Your too funny");
			break;
		default:
			System.out.println("Your age is perfect yo");
			break;	
		}
	}
}
